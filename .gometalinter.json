{
    "Enable": ["deadcode", "golint", "vet", "gotype", "gocyclo", "maligned", "errcheck", "megacheck", "misspell", "unused", "staticcheck"],
    "Exclude": ["test", "fake_*"],
    "Format": "[{{.Severity}}]: {{.Path}}: {{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}} ({{.Linter}})",
    "Deadline": "120s",
    "Aggregate": true,
    "Sort": ["severity", "path", "line", "message"],
    "Checkstyle": false
}